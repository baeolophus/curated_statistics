  shorttitle = {Machine Learning Strategies for Classification and Prediction of Alteration Facies},
  author = {OrdÃ³Ã±ez-CalderÃ³n, Juan C. and Gelcich, Sergio},
  date = {2018-11-01},
  journaltitle = {Journal of Geochemical Exploration},
  shortjournal = {Journal of Geochemical Exploration},
