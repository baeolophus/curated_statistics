	file = {Jackson - 1993 - Stopping Rules in Principal Components Analysis A.pdf:C\:\\Users\\curr0024\\Zotero\\storage\\ZQGWPQQ5\\Jackson - 1993 - Stopping Rules in Principal Components Analysis A.pdf:application/pdf},
