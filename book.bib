@article{burgessAreExperimentSample2022,
  title = {Are Experiment Sample Sizes Adequate to Detect Biologically Important Interactions between Multiple Stressors?},
  author = {Burgess, Benjamin J. and Jackson, Michelle C. and Murrell, David J.},
  date = {2022},
  journaltitle = {Ecology and Evolution},
  volume = {12},
