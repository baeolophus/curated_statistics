@article{peralInappropriateUseTimetoindependence2022,
  title = {The Inappropriate Use of Time-to-Independence Biases Estimates of Activity Patterns of Free-Ranging Mammals Derived from Camera Traps},
  author = {Peral, Christopher and Landman, Marietjie and Kerley, Graham I. H.},
  date = {2022},
  journaltitle = {Ecology and Evolution},
  volume = {12},
