@article{pasaricGeneralisedParetoDistribution2019,
  title = {Generalised {{Pareto}} Distribution: Impact of Rounding on Parameter Estimation},
  shorttitle = {Generalised {{Pareto}} Distribution},
  author = {PasariÄ‡, Z. and CindriÄ‡, K.},
  date = {2019-04},
  journaltitle = {Theoretical and Applied Climatology},
  shortjournal = {Theor Appl Climatol},
