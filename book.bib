@article{boycePopulationSubdivisionDesert1999,
  title = {Population Subdivision among Desert Bighorn Sheep ( {{{\mkbibemph{Ovis}}}} {\mkbibemph{Canadensis}} ) Ewes Revealed by Mitochondrial {{DNA}} Analysis},
