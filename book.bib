@article{rubinInconsistentMultipleTesting2024,
  title = {Inconsistent Multiple Testing Corrections: {{The}} Fallacy of Using Family-Based Error Rates to Make Inferences about Individual Hypotheses},
  shorttitle = {Inconsistent Multiple Testing Corrections},
  author = {Rubin, Mark},
  date = {2024-11-01},
  journaltitle = {Methods in Psychology},
  shortjournal = {Methods in Psychology},
  volume = {10},
  pages = {100140},
  issn = {2590-2601},
  doi = {10.1016/j.metip.2024.100140},
  url = {https://www.sciencedirect.com/science/article/pii/S2590260124000067},
  urldate = {2024-04-17},
  abstract = {During multiple testing, researchers often adjust their alpha level to control the familywise error rate for a statistical inference about a joint union alternative hypothesis (e.g., â€œH1,1 or H1,2â€). However, in some cases, they do not make this inference. Instead, they make separate inferences about each of the individual hypotheses that comprise the joint hypothesis (e.g., H1,1 and H1,2). For example, a researcher might use a Bonferroni correction to adjust their alpha level from the conventional level of 0.050 to 0.025 when testing H1,1 and H1,2, find a significant result for H1,1 (p~{$<~$}0.025) and not for H1,2 (p~{$>~$}0.025), and so claim support for H1,1 and not for H1,2. However, these separate individual inferences do not require an alpha adjustment. Only a statistical inference about the union alternative hypothesis â€œH1,1 or H1,2â€ requires an alpha adjustment because it is based on â€œat least oneâ€ significant result among the two tests, and so it refers to the familywise error rate. Hence, an inconsistent correction occurs when a researcher corrects their alpha level during multiple testing but does not make an inference about a union alternative hypothesis. In the present article, I discuss this inconsistent correction problem, including its reduction in statistical power for tests of individual hypotheses and its potential causes vis-Ã -vis error rate confusions and the alpha adjustment ritual. I also provide three illustrations of inconsistent corrections from recent psychology studies. I conclude that inconsistent corrections represent a symptom of statisticism, and I call for a more nuanced inference-based approach to multiple testing corrections.},
  keywords = {04. Interpretation and meaning: multiple comparisons,04. Interpretation and meaning: significance},
  file = {C:\Users\curr0024\Zotero\storage\Z5D69DU9\S2590260124000067.html}
}

