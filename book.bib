  shorttitle = {{{OpenMEE}}},
  author = {Wallace, Byron C. and Lajeunesse, Marc J. and Dietz, George and Dahabreh, Issa J. and Trikalinos, Thomas A. and Schmid, Christopher H. and Gurevitch, Jessica},
  date = {2017},
  journaltitle = {Methods in Ecology and Evolution},
