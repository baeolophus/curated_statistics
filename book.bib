	pages = {e9289},
	file = {Full Text PDF:C\:\\Users\\curr0024\\Zotero\\storage\\LJWXLY5U\\Burgess et al. - 2022 - Are experiment sample sizes adequate to detect bio.pdf:application/pdf;Snapshot:C\:\\Users\\curr0024\\Zotero\\storage\\2HZ6SGH6\\ece3.html:text/html},
